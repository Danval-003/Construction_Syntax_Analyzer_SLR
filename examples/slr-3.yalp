/* Configuración del parser para Gramática No.3 */
/* Sigue la gramática SLR: */
/* E -> T + E | T */
/* T -> number * T | number | ( E ) */

%token NUMBER
%token PLUS
%token TIMES
%token LPAREN RPAREN
%token WHITESPACE
IGNORE WHITESPACE

expression:
    term PLUS expression
  | term
;
term:
    NUMBER TIMES term
  | NUMBER
  | LPAREN expression RPAREN
;