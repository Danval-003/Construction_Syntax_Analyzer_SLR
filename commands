# primer intento
python .\yal.py .\examples\adaptativeSlr-1.yal -o .\scaner\out_tkl1.py
python .\yapar.py .\examples\slr-1.yalp -o .\out\yaPar.pkl
python .\compiler.py .\trys\try-1.txt -scan .\scaner\out_tkl1.py -oScan .\out\tokens.txt -yapar .\out\yaPar.pkl

